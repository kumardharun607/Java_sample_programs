class  fizz_buzz_by_rec
{
	public static void main(String[] args) 
	{
		int data=1;
		rec(data);
	}
	public static void rec(int data)
	{
		if(data<=100)
		{
			if(data%3==0 && data%5==0)
			{
				System.out.print(data+" "+"Fizz-Buzz");
			}
			else if(data%3==0)
			{
				System.out.println(data+" "+"Fizz");
			}
			else if(data%5==0)
			{
				System.out.print(data+" "+"Buzz");
			}
			
			data++;
			rec(data);
		}
	}
}
