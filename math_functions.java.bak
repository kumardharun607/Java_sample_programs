class math_functions

  
{
	public static void main(String[] args) 
	{
		System.out.println("Math.abs(x)-Absolute value of x =-20:"+Math.abs(-20));
		System.out.println("Math.max(a,b) - Maximum of two 10,5  numbers"+Math.max(10,5));
		System.out.println("Math.min(a,b) - Minimum of two 10,5 numbers"+Math.min(10,5));
		System.out.println("Math.sqrt(x) - Square root of x=16:"+Math.sqrt(16));
		System.out.println("Math.cbrt(x) - Cube root of  x=8:"+Math.cbrt(8));
		System.out.println("Math.pow(a,b) - a raised to the power b  pow(2,3) 2 raised to the power 3 :"+Math.pow(2.0,3.0));
		System.out.println("Math.cbrt(x) - Cube root of  x=8:"+Math.cbrt(8));
		System.out.println("Math.round(x) - Rounds to nearest integer x=2.3"+Math.round(2.3));
        System.out.println("Math.ceil(x) - Rounds UP to nearest integer x=2.3"+Math.ceil(2.3));
        System.out.println("Math.floor(x) - Rounds DOWN to nearest integer x=2.3"+Math.floor(2.3));
        System.out.println("Math.rint(x) - Returns nearest integer (as double) x=2.3"+Math.rint(2.3));
		System.out.println("Math.random() - Random number (0.0 to 1.0)"+Math.random());
        System.out.println("Math.PI - Constant value of p (3.14159)"+Math.PI);
        System.out.println("Math.E - Constant value of e (2.71828)"+Math.E);
		
		
		
	}
}
