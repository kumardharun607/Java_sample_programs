class string8 
{
	public static void main(String[] args) 
	{
		String s="Hi@how#you";
		char a[]=s.toCharArray();
		int l=0;
		int r=s.length()-1;
		while (l<r)
		{
			if (Character.isLetter(a[l]) && Character.isLetter(a[r]))
			{
				char temp=a[l];
				a[l]=a[r];
				a[r]=temp;
				l++;
				r--;
			}
			else if (Character.isLetter(a[l]))
			{
				l++;
			}
			else
			{
				r--;
			}
		}
		System.out.println(new String(a));
	}
}
