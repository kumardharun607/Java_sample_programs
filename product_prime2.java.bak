import java.util.*;
class product_prime2 
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("n=");
		int n=sc.nextInt();
		int count =0;
		int product=1;
		
		for (int i=2;i<=Integer.MAX_VALUE ;i++ )
		{
			
			
			if (checkprime(i))
			{
				product*=i;
				count++;
			if (count<=n)
			{
				
				if (i==0)
				{
					System.out.print(i);
			    }
			    else
			    {
					System.out.print("X"+i);
			    }
					
			}
			if (count==5)
			{
			
			
			System.out.print("="+product);
			return;
			}
			
		}
		}
	}
	public static boolean checkprime(int n)
	{
		if (n==0||n==1)
		{
			return false;
		}
		for (int i=2;i<n ;i++ )
		{
			if (n%i==0)
			{
				return false;
			}
		}
		return true;
	}
	
}
