import java.util.*;
class  array_rev
{
	public static void main(String[] args) 
	{
		int a[]={6,3,7,9,8,8,8,3,4,0};
		System.out.println("a:"+Arrays.toString(a));
		int res[]=reverse(a);
		System.out.println("The sum of even number is:"+Arrays.toString(res));
		
	}
	public static int[] reverse(int []x)
	{
		int l=0,r=x.length-1;
		while(l<r)
		{
			int temp=x[l];
			x[l]=x[r];
			x[r]=temp;
			l++;
			r--;
		}
		return x;
	}
}
