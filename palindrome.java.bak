import java.util.*;
class  palindrome
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("Enter a value:");
		int num=sc.nextInt();
		int rev=0;
		int temp=num;
		while(num>0)
		{
			rev=rev*10+num%10;
			num=num/10;
		}
		if(temp==rev)
		{
			System.out.println("the number is palindrome");
		}
		else
		{
			System.out.println("the number is palindrome");
		}
	}
}
