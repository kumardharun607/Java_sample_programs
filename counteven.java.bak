import java.util.*;
class  counteven
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.println("Enter a number:");
		int num=sc.nextInt();
		
		int sum=0;
		while(num>0)
		{
			int last=num/10;
			if(last%2==0)
			{
				sum+=last;
			}
			num=num/10;
		}
		System.out.println(sum);
	}
}
