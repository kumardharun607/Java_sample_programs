class pattern2 
{
	public static void main(String[] args) 
	{
		int n=5;
		for (int i=0;i<n ;i++ )//rows
		{
			for (int j=0;j<n ;j++ )//coloumns
			{
				  //------downwardown  ----forward up
				if (j==i-1 ||j==i-2 || j==i+1||j==i+3)
				{
					System.out.print("* ");
				}
				else
				{
					System.out.print("@ ");
				}
			}
			System.out.println();
		}
	}
}
