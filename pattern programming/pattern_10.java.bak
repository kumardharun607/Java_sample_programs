class pattern_10 
{
	public static void main(String[] args) 
	{
		int n=5;
		char c='A';
		for (int i=0;i<n ;i++ )
		{
			char c='A';
			for (int j=0;j<n ;j++ )
			{
				if (i>=j)
				{
					System.out.print((i%2==0)?(j+1):(c++)+"\t");
				}
				else
				{
					System.out.print(" \t");
				}
				
			}
			System.out.println();
		}
		
	}
}
