import java.util.*;
class array_reverse 
{
	public static void main(String[] args) 
	{
		int []a={6,3,7,9,8,8,8,3,4,0};
		System.out.println("Before reversing"+Arrays.toString(a));
		int l=0;
		int r=a.length-1;
		while(l<r)
		{
			int temp =a[l];
			a[l]=a[r];
			a[r]=temp;
		}
		System.out.println("After reversing"+Arrays.toString(a));
	}
}
