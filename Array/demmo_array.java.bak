import java.util.*;
class demmo_array 
{
	public static void main(String[] args) 
	{
		String s="abcdefg";
		System.out.println(reverse(s,s.length()));
		

	}
	public static String reverse(String s,int count)
	{
		
		
		
		if(count>0)
		{
			s+=s.charAt(s.length()-1);
			count--;
			reverse(s.substring(0,s.length()-1),count);
		}
		return s;
	}
}
