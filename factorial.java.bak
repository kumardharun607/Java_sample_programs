import java.util.*;
class  factorial
{
	static Scanner sc=new Scanner (System.in);
	public static void main(String[] args) 
	{
		System.out.println("Enter a number:");
		int num=sc.nextInt();
		
		System.out.println(fact(num));
		
	}
	public static int fact(int n)
	{
		int facts=1;
		for(int i=2;i<=n;i++)
		{
			facts=facts*i;
		}
		return facts;
			
	}
				
		
		
    
}
