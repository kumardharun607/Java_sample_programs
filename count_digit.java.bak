import java.util.*;
class  count_digit
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.println("Enter a number:");
		String num=sc.nextLine();
		int count=0;
		int i=0;
		while(i<num.length()+1)
		{
			
			count+=1;
			i++;
		}
		System.out.println("Total digit is:"+count);
	}
}
 
