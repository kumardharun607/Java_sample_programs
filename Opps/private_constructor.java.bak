class private_constructor 
{
	private String name;
	private int id; //states
	
	private private_constructor(String name,int id)
	{
		this.name=name;
		this.id=id;
	}
	
	public void getdetails()
	{
		System.out.println("Name:"+name);
				System.out.println("Id:"+id);
	}
	
	public private_constructor getconstructorobj(String name,int id)
	{
		return new private_constructor(name,id);
	}


}
