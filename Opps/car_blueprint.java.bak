class car_blueprint 
{
	String name;
	String color;
	double price;
	
	Engine e;
	
	car_blueprint(String name,String color,double price,Engine e)
	{
		this.name=name;
		this.color=color;
		this.price=price;
		this.e=e;
	}
	
	public void details_of_car()
	{
		System.out.println("Car name:"+name);
		System.out.println("Car color:"+color);
		System.out.println("Car price:"+price);
	}

}
