import java.util.*;
class  sum_of_n
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		int sum=0;
		System.out.println("Enter the value for n:");
		int n=sc.nextInt();
		for(int i=1;i>=n;i++)
		{
			sum=sum+i;
		}
		System.out.println("Sum of n natural number is:"+sum);
	}
}
